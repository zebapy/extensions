{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "viacep",
  "title": "ViaCEP",
  "description": "Find Brazilian addresses and postal codes",
  "icon": "viacep-icon.png",
  "author": "xilopaint",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "keywords": [
    "cep",
    "brazil"
  ],
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "find-address",
      "title": "Find Address",
      "subtitle": "ViaCEP",
      "description": "Search address by CEP",
      "mode": "view",
      "keywords": ["cep"],
      "arguments": [
        {
          "name": "cep",
          "type": "text",
          "required": false,
          "placeholder": "CEP"
        }
      ]
    },
    {
      "name": "find-cep",
      "title": "Find CEP",
      "subtitle": "ViaCEP",
      "description": "Search CEP by address",
      "mode": "view",
      "keywords": ["address"]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.8",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
