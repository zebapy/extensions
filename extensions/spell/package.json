{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "spell",
  "title": "Spell",
  "description": "Struggling to spell a word? With this extension, even with typos, you can type the word and receive a list of matching suggestions using the OneLook API.",
  "icon": "extension_icon.png",
  "author": "Gorzog",
  "contributors": [
    "gezerwezer"
  ],
  "categories": [
    "Productivity",
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Spell",
      "description": "Uses OneLook API to get a list of words that match the given word.",
      "mode": "view",
      "preferences": [
        {
          "name": "language",
          "title": "Language",
          "description": "Choose the language for word searches.",
          "type": "dropdown",
          "required": false,
          "defaultValue": "en",
          "data": [
            { "title": "English", "value": "en" },
            { "title": "Spanish", "value": "es" }
          ]
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.19.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}

