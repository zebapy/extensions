{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "react-docs",
  "title": "React Documentation",
  "description": "Quick links to the new React documentation",
  "icon": "icon.png",
  "author": "webdiego",
  "contributors": [
    "0xdhrv"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open Documentation",
      "subtitle": "React",
      "description": "Quick links to the new React documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.7",
    "eslint": "^9.39.1",
    "prettier": "^3.7.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "update-docs": "tsx scripts/update-docs.ts",
    "type-check": "tsc --noEmit"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}
