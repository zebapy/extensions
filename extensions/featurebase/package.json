{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "featurebase",
  "title": "Featurebase",
  "description": "The next-gen support & feedback platform for modern teams",
  "icon": "extension_icon.png",
  "author": "xmok",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "organization_subdomain",
      "title": "Organization Subdomain",
      "description": "Featurebase Organization Subdomain",
      "placeholder": "example",
      "required": true,
      "type": "textfield"
    },
    {
      "name": "api_key",
      "title": "API Key",
      "placeholder": "fb-xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
      "description": "Featurebase API Key",
      "required": true,
      "type": "password"
    }
  ],
  "commands": [
    {
      "name": "manage-changelogs",
      "title": "Manage Changelogs",
      "subtitle": "Featurebase",
      "description": "Manage Featurebase Changelogs",
      "mode": "view"
    },
    {
      "name": "manage-help-center",
      "title": "Manage Help Center",
      "subtitle": "Featurebase",
      "description": "Manage Featurebase Help Center",
      "mode": "view"
    },
    {
      "name": "manage-feedback",
      "title": "Manage Feedback",
      "subtitle": "Featurebase",
      "description": "Manage Featurebase Feedback",
      "mode": "view"
    },
    {
      "name": "manage-users",
      "title": "Manage Users",
      "subtitle": "Featurebase",
      "description": "Manage Featurebase Users",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.6",
    "@raycast/utils": "^2.2.2",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/turndown": "^5.0.6",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
